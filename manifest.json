{
    "manifest_version": 2,
    "name": "WarframeItemInfo",
    "version": "1.0",
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "images/warframe_logo-32x.png",
        "default_title": "Click to Toggle"
      },
      "permissions": [
        "activeTab",
        "*://*/*",
        "tabs",
        "storage"
      ]
}